<odoo>

  <record id="view_company_form_extended" model="ir.ui.view">
    <field name="name">res.company.form.extended</field>
    <field name="model">res.company</field>
    <field name="inherit_id" ref="base.view_company_form"/>
    <field name="arch" type="xml">
      <xpath expr="//page[last()]" position="after">
        <page string="Trading License Information" name="trading_license_info">
          <field name="trade_license_number_ids">
            <tree editable="top">
              <field name="company_id" invisible="1"/>
              <field name="name" />
            </tree>
          </field>
        </page>
      </xpath>
    </field>
  </record>

</odoo>
